//alien object
function alien()
 add(aliens,{
  x=pos.x,
  y=pos.y,
  spawned=false,
  e_ang=0,
  health=0,
  w=12,
  h=13,
  anim_speed=6,
  attack_speed=20,
  anim_timer=0,
  hurt=false,
  hurt_speed=2,
  hurt_timer=0,
  attacking=false,
  
  draw=function(self)
   pal()
   palt(14,true)
   palt(0,false)
   if (self.spawned==true) then
    if (self.hurt) then
    	pal(3,8)
    	self.hurt=false;
    end
			
				if (self.attacking) then
					if self.anim_timer<self.attack_speed then
	     spr(10,self.x,self.y,2,2)
	     if self.anim_timer==0 then
	      hp-=100
	     end
	     self.anim_timer+=1
	    else
	     spr(8,self.x,self.y,2,2)
	     self.anim_timer+=1
	    end
				else
	    if self.anim_timer<self.anim_speed then
	     spr(6,self.x,self.y,2,2)
	     self.anim_timer+=1     
	    else
	     spr(8,self.x,self.y,2,2)
	     self.anim_timer+=1
	    end
	   end
    
    if self.anim_timer>self.anim_speed*2 then
     if self.attacking then 
      if self.anim_timer>self.attack_speed*2 then
       self.anim_timer=0
      end
     else
      self.anim_timer=0
     end
    end
   end
  end,
  
  update=function(self)
   self:spawn(self)
   
   if(self.health > 0) then
	   dx=-cos(self.e_ang)*0.5
	   dy=-sin(self.e_ang)*0.5
	   
	   if (self.attacking==false) then
	   	self.x+=dx
	   	self.y+=dy
	   end
	  else
	   del(aliens, self)
	   cur_enemies-=1
	   points+=100
   end
  end,
  
  spawn=function(self)
   if self.spawned==false then
    check=true
    side=flr(rnd(5))+0
    num=side
    
    if (side==1) then
     self.y=-16
     self.x=-16+rnd(144)
    elseif (side==2) then
     self.x=144
     self.y=-16+rnd(144)
    elseif (side==3) then
     self.y=144
     self.x=-16+rnd(144)
    else
     self.x=-16
     self.y=-16+rnd(144)
    end
    
    //gets the angle of the enemy
    //in reference to the player
    self.e_ang=atan2(self.x-60, self.y-60)
    
    self.health=round*100
    
    self.spawned=true
   end
  end,
 })
end


//randomly spawns aliens
function spawn(e)
 //spawns aliens based on round
 if enemies<=0 then
  //checks to see if any enemies
  //are still alive
  if cur_enemies <= 0 then
   if round==0 then
		  round+=1
		  enemies=sqrt(round)*10
		 else
		  next_round=true
		  if btn(❎) then
			  round+=1
			  enemies=sqrt(round)*10
			  next_round=false
		  end
		  if btn(🅾️) then
			  scene=4
			  next_round=false
		  end
		 end
  end
 end
 
 if cur_enemies < max_enemies then
	 if sp_timer<=0 and enemies > 0 then
	  sp_timer=20
	  cur_enemies+=1
	  alien()
		 enemies-=1
	 else
	  sp_timer-=1
	 end
 end
end


function attacking()
 for a in all(aliens) do
  if
	 (a.x > pos.x+w+2) 
		or
		(a.x+a.w < pos.x+2) 
		or
		(a.y > pos.y+h) 
		or
		(a.y+a.h < pos.y) 
		then
		else
		 if a.attacking==false then
		  hp-=100
		 end
		 a.attacking=true
		end
 end
end
