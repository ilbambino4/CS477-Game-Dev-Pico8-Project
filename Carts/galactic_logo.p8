pico-8 cartridge // http://www.pico-8.com
version 38
__lua__
timer=0
middle=true
fcount=0
scene=1
function logo()
 local cx = 54
 local cy = 45
 
 if timer<=5 and middle then
  spr(74,cx,cy,2,2)
  timer+=1
 elseif timer<10 and middle then
  spr(76,cx,cy,2,2)
  timer+=1
 elseif timer<15 and middle then
  spr(78,cx,cy,2,2)
  timer+=1
 else
  spr(76,cx,cy,2,2)
  middle=false
  if timer <= 10 then
   timer=0
   middle=true
  end
  timer-=1
 end
 
 if(fcount > 60) then
  print("galactic",47,cy+20,8)
 end
 if(fcount > 120) then
  print("games",53,cy+26,13)
 end
 
 if(fcount >= 240) then
  scene+=1
 end
 fcount+=1
end

function _update()
 fcount += 1
end

function _draw()
 cls()
 if scene==1 then
  logo()
 else
  print("press ‚ùé to start",32,55,8)
 end
end
